#: ../lib/pytis/data/access.py:434
msgid "Access denied"
msgstr "Neoprávněný přístup"

#: ../lib/pytis/data/dbapi.py:83
msgid "Can't connect to database"
msgstr "Nemohu se připojit do databáze"

#: ../lib/pytis/data/dbapi.py:171
msgid "Database interface error"
msgstr "Chyba v databázovém rozhraní"

#: ../lib/pytis/data/dbapi.py:187
msgid "Database connection timeout"
msgstr "Vypršel časový limit pro navázání databázového spojení"

#: ../lib/pytis/data/dbapi.py:189 ../lib/pytis/data/dbapi.py:299
msgid "Database connection error"
msgstr "Chyba databázového spojení"

#: ../lib/pytis/data/dbapi.py:203 ../lib/pytis/data/pgsql.py:87
#: ../lib/pytis/data/pgsql.py:94
msgid "Database operational error"
msgstr "Chyba databázové operace"

#: ../lib/pytis/data/dbapi.py:207 ../lib/pytis/data/pgsql.py:99
msgid "Database integrity violation"
msgstr "Porušení integrity dat v databázi"

#: ../lib/pytis/data/dbdata.py:299
msgid "Too many database connections"
msgstr "Příliš mnoho databázových spojení"

#: ../lib/pytis/data/dbdata.py:707
msgid "Database error"
msgstr "Databázová chyba"

#: ../lib/pytis/data/dbdata.py:775
msgid "Invalid user name or password"
msgstr "Chybné uživatelské jméno nebo heslo"

#: ../lib/pytis/data/pgsql.py:60
msgid "Unable to connect to the database"
msgstr "Nelze se připojit k databázi"

#: ../lib/pytis/data/pgsql.py:69
msgid "Query arguments unsupported by the pgsql backend"
msgstr "Argumenty dotazu nejsou podporovány při použití pgsql backendu"

#: ../lib/pytis/data/postgresql.py:275 ../lib/pytis/data/postgresql.py:361
#: ../lib/pytis/data/postgresql.py:373
msgid "Method not implemented"
msgstr "Volána neimplementovaná metoda"

#: ../lib/pytis/data/postgresql.py:508
msgid "Characters present in data not representable in database encoding"
msgstr "Data obsahují znaky, které nelze reprezentovat v kódování databáze"

#: ../lib/pytis/data/postgresql.py:3690
msgid "Invalid database counter value"
msgstr "Chybná hodnota čítače z databáze"

