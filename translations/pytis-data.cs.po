#: ../lib/pytis/data/access.py:432
msgid "Access denied"
msgstr "Neoprávněný přístup"

#: ../lib/pytis/data/dbapi.py:87
msgid "Waiting for server recovery"
msgstr "Čekám na zotavení serveru"

#: ../lib/pytis/data/dbapi.py:99
msgid "Can't connect to database"
msgstr "Nemohu se připojit do databáze"

#: ../lib/pytis/data/dbapi.py:227
msgid "Database interface error"
msgstr "Chyba v databázovém rozhraní"

#: ../lib/pytis/data/dbapi.py:246
msgid "Database connection timeout"
msgstr "Vypršel časový limit pro navázání databázového spojení"

#: ../lib/pytis/data/dbapi.py:248 ../lib/pytis/data/dbapi.py:383
msgid "Database connection error"
msgstr "Chyba databázového spojení"

#: ../lib/pytis/data/dbapi.py:262
msgid "Database operational error"
msgstr "Chyba databázové operace"

#: ../lib/pytis/data/dbapi.py:270
msgid "Database internal error"
msgstr "Interní databázová chyba"

#: ../lib/pytis/data/dbapi.py:274
msgid "Database integrity violation"
msgstr "Porušení integrity dat v databázi"

#: ../lib/pytis/data/dbdata.py:292
msgid "Too many database connections"
msgstr "Příliš mnoho databázových spojení"

#: ../lib/pytis/data/dbdata.py:722
msgid "Database error"
msgstr "Databázová chyba"

#: ../lib/pytis/data/dbdata.py:791
msgid "Invalid user name or password"
msgstr "Chybné uživatelské jméno nebo heslo"

#: ../lib/pytis/data/postgresql.py:426 ../lib/pytis/data/postgresql.py:536
#: ../lib/pytis/data/postgresql.py:548
msgid "Method not implemented"
msgstr "Volána neimplementovaná metoda"

#: ../lib/pytis/data/postgresql.py:3547
msgid "Invalid database counter value"
msgstr "Chybná hodnota čítače z databáze"

#: ../lib/pytis/data/types_.py:371
#, python-format
msgid "Wildcard matching not supported for values of type '%s'."
msgstr "Hvězdičkové výrazy nejsou podporovány pro hodnoty typu '%s'."

#. User input validation error message.
#: ../lib/pytis/data/types_.py:378
msgid "Empty value"
msgstr "Prázdná hodnota"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:394
msgid "Invalid value"
msgstr "Neplatná hodnota"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:542
#, python-format
msgid "Minimal value is %(minimum)s"
msgstr "Minimální hodnota je %(minimum)s"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:545
#, python-format
msgid "Maximal value is %(maximum)s"
msgstr "Maximální hodnota je %(maximum)s"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:585
#, python-format
msgid "Minimal size %(minlen)s not satisfied"
msgstr "Nedodržena minimální velikost %(minlen)s"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:587
#, python-format
msgid "Maximal size %(maxlen)s exceeded"
msgstr "Maximální velikost %(maxlen)s překročena"

#: ../lib/pytis/data/types_.py:738
msgid "Lower range bound higher than the upper one"
msgstr "Dolní hranice rozsahu je vyšší než horní"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:831
msgid "Not an integer"
msgstr "Není to celé číslo"

#: ../lib/pytis/data/types_.py:1003
msgid "Invalid number"
msgstr "Není to povolené číslo"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:1136
#, python-format
msgid "Minimal length %(minlen)s characters not satisfied"
msgstr "Nedodržena minimální délka %(minlen)s znaků"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:1138
#, python-format
msgid "Maximal length %(maxlen)s characters exceeded"
msgstr "Řetězec přesahuje maximální délku %(maxlen)s znaků"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:1264
msgid "Please use mix of letters and non-letters in your password"
msgstr "Použijte ve svém hesle kombinaci písmenných a nepísmenných znaků"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:1270
msgid "Enter the password twice to eliminate typos"
msgstr "Zadejte heslo dvakrát pro eliminování překlepů"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:1273
msgid "Passwords don't match"
msgstr "Kontrolní zadání hesla neodpovídá"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:1296
msgid "Invalid MD5 hash"
msgstr "Neplatná MD5 suma"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:1303 ../lib/pytis/data/types_.py:1341
#: ../lib/pytis/data/types_.py:1371 ../lib/pytis/data/types_.py:1391
#: ../lib/pytis/data/types_.py:2110
msgid "Invalid format"
msgstr "Neplatný formát"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:1326
msgid "Invalid color format ('#RGB' or '#RRGGBB')"
msgstr "Formát barvy neodpovídá ('#RGB' nebo '#RRGGBB')"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:1346
#, python-format
msgid "Invalid inet address mask: %(mask)s"
msgstr "Neplatná maska inet adresy: %(mask)s"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:1353
#, python-format
msgid "Invalid inet address value %(addr)s"
msgstr "Neplatná hodnota inet adresy: %(addr)s"

#: ../lib/pytis/data/types_.py:1518
msgid "Invalid date or time format"
msgstr "Chybný formát data nebo času"

#: ../lib/pytis/data/types_.py:1725
msgid "Date outside the allowed range"
msgstr "Datum mimo povolený rozsah"

#: ../lib/pytis/data/types_.py:2224
msgid "Invalid boolean type input value"
msgstr "Neplatná vstupní hodnota typu boolean"

#: ../lib/pytis/data/types_.py:2415
msgid "Invalid graphics format"
msgstr "Neplatný grafický formát"

#: ../lib/pytis/data/types_.py:2473
#, python-format
msgid "Minimal pixel size %(minsize)s exceeded"
msgstr "Minimální rozměr %(minsize)s pixelů nedodržen"

#: ../lib/pytis/data/types_.py:2476
#, python-format
msgid "Maximal pixel size %(maxsize)s exceeded"
msgstr "Maximální rozměr %(maxsize)s pixelů překročen"

#: ../lib/pytis/data/types_.py:2480
#, python-format
msgid "Unsupported format %(format)s; valid formats: %(formats)s"
msgstr "Nepodporovaný formát %(format)s; platné formáty: %(formats)s"

#: ../lib/pytis/data/types_.py:2527
msgid "Invalid hierarchical value format"
msgstr "Chybný formát hierarchické hodnoty"

#: ../lib/pytis/data/types_.py:2529
msgid "One of hierarchical value items is too long"
msgstr "Jedna z položek hierarchické hodnoty je příliš dlouhá"

#: ../lib/pytis/data/types_.py:2531
msgid "One of hierarchical value items contains invalid characters"
msgstr "Jedna z položek hierarchické hodnoty obsahuje neplatné znaky"

#: ../lib/pytis/presentation/field.py:482
msgid "Such value already exists."
msgstr "Taková hodnota již existuje"

#: ../lib/pytis/presentation/field.py:745
msgid "unlimited"
msgstr "neomezeno"

