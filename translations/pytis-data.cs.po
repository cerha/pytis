#: ../lib/pytis/data/types_.py:2802
msgid "Not a JSON string"
msgstr "Není JSON řetězec"

#: ../lib/pytis/data/types_.py:2806
msgid "Unsupported JSON value"
msgstr "Nepodporovaná JSON hodnota"

#: ../lib/pytis/data/types_.py:2812
msgid "JSON value doesn't match schema definition"
msgstr "JSON hodnota neodpovídá definici schematu"

#: ../lib/pytis/presentation/field.py:484
msgid "Such value already exists."
msgstr "Taková hodnota již existuje"

#: ../lib/pytis/presentation/field.py:759
msgid "unlimited"
msgstr "neomezeno"

