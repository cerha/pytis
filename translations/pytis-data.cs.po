msgid "Lower range bound higher than the upper one"
msgstr "Dolní hranice rozsahu je vyšší než horní"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:868
msgid "Not an integer"
msgstr "Není to celé číslo"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:1008
msgid "Invalid number"
msgstr "Není to povolené číslo"

#. User input validation error message.
#: ../lib/pytis/data/types_.py:1194
#, python-format
