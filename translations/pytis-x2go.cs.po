#: ../bin/x2goclient.py:1351 ../bin/x2goclient2.py:1348
msgid "Creating new session. Please wait..."
msgstr "Vytváření nového sezení. Vyčkejte prosím..."

#: ../bin/x2goclient.py:1354 ../bin/x2goclient2.py:1351
msgid "Setting up new session. Please wait..."
msgstr "Nastavování nového sezení. Vyčkejte prosím..."

#: ../bin/x2goclient.py:1380 ../bin/x2goclient2.py:1377
msgid "New session"
msgstr "Nové sezení"

#: ../bin/x2goclient.py:1382 ../bin/x2goclient2.py:1379
msgid "OK"
msgstr "Budiž"

#: ../bin/x2goclient.py:1382 ../bin/x2goclient2.py:1379
msgid "Terminate"
msgstr "Ukončit"

#: ../bin/x2goclient.py:1382 ../bin/x2goclient2.py:1379
msgid "Cancel"
msgstr "Zrušit"

#: ../bin/x2goclient.py:1383 ../bin/x2goclient2.py:1380
msgid "Resume session"
msgstr "Obnova sezení"

#: ../bin/x2goclient.py:1463 ../bin/x2goclient2.py:1460
msgid "Create desktop shortcut for this session profile?"
msgstr "Vytvořit pro tento profil ikonu na ploše?"

#: ../bin/x2goclient.py:1472 ../bin/x2goclient2.py:1469
#, python-format
msgid "Shortcut %s allready exists. Please, rename it:"
