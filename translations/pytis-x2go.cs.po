#: ../bin/x2goclient.py:250
msgid "Can't read RPyC info file: %s"
msgstr "Nelze načíst soubor s informacemi pro RPyC: %s"

#: ../bin/x2goclient.py:260
msgid "Error when writing RPyC info file: %s"
msgstr "Chyba při zápisu souboru s informacemi pro RPyC: %s"

#: ../bin/x2goclient.py:281 ../bin/x2goclient.py:1025
msgid "Invalid broker address"
msgstr "Chybná adresa brokeru"

#: ../bin/x2goclient.py:285 ../bin/x2goclient.py:1029
msgid "Unsupported broker protocol"
msgstr "Nepodporovaný protokol pro přístup na broker"

#: ../bin/x2goclient.py:410
msgid "Invalid session format"
msgstr "Chybný formát sezení"

#: ../bin/x2goclient.py:601
msgid "New pytis client version available. Install?"
msgstr "Je dostupná nová verze klienta Pytis. Nainstalovat?"

#: ../bin/x2goclient.py:670
msgid "Select session"
msgstr "Vyberte sezení"

#: ../bin/x2goclient.py:674
msgid "No session selected."
msgstr "Nebylo vybráno žádné sezení."

#: ../bin/x2goclient.py:810
msgid "No free port found for RPyC in the range %s-%s"
msgstr "Nenalezen volný port pro RPyC v rozsahu %s-%s"

#: ../bin/x2goclient.py:937
msgid "Password key for %s"
msgstr "Heslo ke klíči v souboru %s"

#: ../bin/x2goclient.py:983
msgid "Login using password"
msgstr "Přihlášení heslem"

#: ../bin/x2goclient.py:984
msgid "Login using a key file"
msgstr "Přihlášení klíčem"

#: ../bin/x2goclient.py:985
msgid "Default authentication failed"
msgstr "Výchozí autentizace selhala"

#: ../bin/x2goclient.py:993
msgid "No supported ssh connection method available"
msgstr "Není dostupná žádná metoda spojení přes ssh"

#: ../bin/x2goclient.py:996
msgid "Login password"
msgstr "Přihlášovací heslo"

#: ../bin/x2goclient.py:1003
msgid "Select ssh key file"
msgstr "Vyberte ssh klíč"

#: ../bin/x2goclient.py:1013
msgid "Program error"
msgstr "Chyba programu"

#: ../bin/x2goclient.py:1037
msgid "Couldn't connect to upgrade server"
msgstr "Nelze se připojit na aktualizační server"

#: ../bin/x2goclient.py:1047
msgid "Package unpacking failed"
msgstr "Selhalo rozbalení balíku"

#: ../bin/x2goclient.py:1053
msgid "Pytis successfully upgraded. Restart the application."
msgstr "Pytis byl úspěšně aktualizován. Restartujte aplikaci."

#: ../bin/x2goclient.py:1100
msgid "Create desktop shortcut for this session profile?"
msgstr "Vytvořit pro tento profil ikonu na ploše?"

#: ../bin/x2goclient.py:1110
msgid "Shortcut %s allready exists. Choose another name:"
msgstr "Zástupce %s již existuje. Zvolte jiné jméno:"

#: ../bin/x2goclient.py:1111
msgid "Edit shortcut name"
msgstr "Upravit jméno zástupce"

