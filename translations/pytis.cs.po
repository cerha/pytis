#: ../lib/pytis/web/form.py:930
msgid "Previous"
msgstr "Předchozí"

#: ../lib/pytis/web/form.py:931
msgid "Go to previous page"
msgstr "Přejít na předchozí stránku výpisu"

#: ../lib/pytis/web/form.py:932
msgid "Next"
msgstr "Další"

#: ../lib/pytis/web/form.py:933
msgid "Go to next page"
msgstr "Přejít na následující stránku výpisu"

#: ../lib/pytis/web/form.py:938 ../lib/pytis/web/form.py:940
msgid "Records per page"
msgstr "Počet záznamů na stránku"

#: ../lib/pytis/web/form.py:945
msgid "Go"
msgstr "Jdi"
