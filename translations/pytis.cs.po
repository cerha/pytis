msgstr "Strana"

#: ../lib/pytis/web/form.py:1255
msgid "Previous"
msgstr "Předchozí"

#: ../lib/pytis/web/form.py:1256
msgid "Go to previous page"
msgstr "Přejít na předchozí stránku výpisu"

#: ../lib/pytis/web/form.py:1257
msgid "Next"
msgstr "Další"

#: ../lib/pytis/web/form.py:1258
msgid "Go to next page"
msgstr "Přejít na následující stránku výpisu"

#: ../lib/pytis/web/form.py:1263 ../lib/pytis/web/form.py:1265
msgid "Records per page"
msgstr "Počet záznamů na stránku"

#: ../lib/pytis/web/form.py:1270
msgid "Go"
msgstr "Jdi"

